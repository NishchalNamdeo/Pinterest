<% include ./partials/header.ejs %>
<div class="w-full px-10 py-8 text-white min-h-screen bg-zinc-800">
    <div class="cards flex flex-wrap gap-10">
     <% posts.forEach(elem => { %>
        <div class="card w-32">
            <div class=" w-full h-40 bg-sky-200 rounded-lg overflow-hidden">
                <img class="w-full h-full object-cover" src="/images/uploads/<%= elem.image %>" alt="">
            </div>
            <h5 class="text-sm mt-2"><%= elem.title %></h5>
            <h5 class="text-sm mt-2"><%= elem.user.name %></h5>
            <h5 class="text-sm mt-2">@<%= elem.user.username %></h5>
        </div>
      
     <% }) %>
    </div>
</div>
<% include ./partials/footer.ejs %>
